                <link_coulombdamping  idbody1="{idbody1}" idbody2="{idbody2}">
                    <point_fb1 x="{point_fb1[0]}" y="{point_fb1[1]}" z="{point_fb1[2]}" comment="Point in body 1" />
                    <point_fb2 x="{point_fb2[0]}" y="{point_fb2[1]}" z="{point_fb2[2]}" comment="Point in body 2" />
                    <rest_length value="{rest_length}" comment="Spring equilibrium length [m]" />
                    <damping value="{damping}" comment="Coulomb force [N]" />
                    <savevtk>
                        <nside value="{nside}" comment="number of sections for each revolution. 0=not saved, 1=line (default=16)" />
                        <radius value="{radius}" comment="spring radius (default=3)" />
                        <length value="{length}" comment="length for each revolution (default=1)" />
                    </savevtk>
                </link_coulombdamping>